    <!-- 
      ========== 3. 中间区域示例：2行 x 3列停车位 ==========
      每个车位： 0.4m(长) x 0.3m(宽)，木质底色与地面一致，边框白色
      这里示范放在 (x ~ 2.2 ~ 3.0, y ~ 2.0 / 1.6)，可按需修改
    -->
    
    <!-- 车位示例函数式描述：
         - 半长 = 0.2, 半宽 = 0.15
         - 若要相邻，当前车位右边缘 = 下一车位左边缘
         - 例如第1列中心 x=2.2, 第2列 x=2.6, 第3列 x=3.0 (间隔0.4)
         - 第1行 y=2.0, 第2行 y=1.6 (间隔0.4)
    -->

    <!-- spot_1 -->
    <model name="parking_spot_1">
      <static>true</static>
      <pose>0 0.3 0 0 0 0</pose>
      <link name="spot_link">
        <!-- 因地面也为木色，这里若要“看得见车位区域”，可留一浅色或同木色 -->
        <visual name="base_plate">
          <geometry>
            <box>
              <size>0.4 0.3 0.001</size>
            </box>
          </geometry>
          <material>
            <!-- 同样木质黄色，可稍调深浅 -->
            <ambient>0.9 0.7 0.4 1</ambient>
            <diffuse>0.9 0.7 0.4 1</diffuse>
          </material>
        </visual>
        <!-- 四条白线边框 -->
        <visual name="top_border">
          <pose>0 0.15 0.002 0 0 0</pose>
          <geometry>
            <box>
              <size>0.4 0.005 0.001</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
          </material>
        </visual>
        <visual name="bottom_border">
          <pose>0 -0.15 0.002 0 0 0</pose>
          <geometry>
            <box>
              <size>0.4 0.005 0.001</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
          </material>
        </visual>
        <visual name="left_border">
          <pose>-0.2 0 0.002 0 0 0</pose>
          <geometry>
            <box>
              <size>0.005 0.3 0.001</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
          </material>
        </visual>
        <visual name="right_border">
          <pose>0.2 0 0.002 0 0 0</pose>
          <geometry>
            <box>
              <size>0.005 0.3 0.001</size>
            </box>
          </geometry>
          <material>
            <ambient>1 1 1 1</ambient>
            <diffuse>1 1 1 1</diffuse>
          </material>
        </visual>
      </link>
    </model>

