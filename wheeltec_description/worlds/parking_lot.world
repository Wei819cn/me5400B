<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="parking_lot">
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- ========== 1. 木质地面 (wood_ground_plane) ========== -->
    <model name="wood_ground_plane">
      <static>true</static>
      <pose>0 0 0 0 0 0</pose>
      <link name="ground_link">
        <visual name="ground_visual">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <!-- 尺寸可根据需要调大 -->
              <size>20 20</size>
            </plane>
          </geometry>
          <material>
            <!-- 木质黄色 (RGBA) -->
            <ambient>0.9 0.7 0.4 1</ambient>
            <diffuse>0.9 0.7 0.4 1</diffuse>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>20 20</size>
            </plane>
          </geometry>
        </collision>
      </link>
    </model>



